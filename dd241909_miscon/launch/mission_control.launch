<?xml version="1.0"?>

<launch>
    <arg name="navgoal_topic"           default="move_base_simple/goal"/>
    <arg name="plan_path_action"        default="plan_global_path"/>
    <arg name="navigate_action"         default="navigate"/>
    <arg name="base_pose_topic"         default="pose"/>

    <node pkg="dd241909_miscon" type="state_machine_node.py" name="state_machine" output="screen">
        <param name="navgoal_topic"         value="$(arg navgoal_topic)"/>
        <param name="plan_path_action"      value="$(arg plan_path_action)"/>
        <param name="navigate_action"       value="$(arg navigate_action)"/>
        <!--param name="base_pose_topic"       value="$(arg base_pose_topic)"/-->
    </node>
</launch>